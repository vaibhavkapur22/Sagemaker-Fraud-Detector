# AWS Configuration
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_REGION=us-east-1

# SageMaker Configuration
SAGEMAKER_ENDPOINT_NAME=fraud-detector-endpoint

# Set to "true" to use mock predictions (no SageMaker calls)
# Set to "false" to use real SageMaker endpoint
SAGEMAKER_MOCK_MODE=true

# SageMaker Feature Store
FEATURE_GROUP_NAME=fraud-detection-decline-rates

# Redis Configuration (for decline rate tracking)
# Local development
REDIS_URL=redis://localhost:6379/0

# Production (AWS ElastiCache)
# ELASTICACHE_URL=redis://your-elasticache-endpoint:6379
# REDIS_SSL=true
# REDIS_CLUSTER_MODE=false

# PostgreSQL Configuration (Production)
# DATABASE_URL=postgresql://user:password@host:5432/fraud_detector_production
# Or use individual parameters:
# DB_HOST=your-rds-endpoint.region.rds.amazonaws.com
# DB_PORT=5432
# DB_NAME=fraud_detector_production
# DB_USERNAME=postgres
# DB_PASSWORD=your_password

# Webhook Configuration
# Set to "true" to require signature verification on webhooks
WEBHOOK_SIGNATURE_REQUIRED=false
# Secret for webhook signature verification (HMAC-SHA256)
# WEBHOOK_SECRET=your_webhook_secret_here
